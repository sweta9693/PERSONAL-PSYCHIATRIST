<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gratitude"
    tools:context=".Gratitude" >

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:foregroundTint="#151414"
        android:inputType="textPersonName"
        android:text="Date"
        android:textColor="#121111"
        android:textColorHighlight="#0E0D0D"
        android:textColorHint="#131212"
        android:textColorLink="#151414"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.949"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.049" />

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit"
        android:onClick="opendatasaved"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.721"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.903"
        tools:ignore="UsingOnClickInXml" />

    <EditText
        android:id="@+id/editTextTextPersonName4"
        android:layout_width="573dp"
        android:layout_height="123dp"
        android:ems="10"
        android:fontFamily="sans-serif-light"
        android:inputType="textMultiLine|textPersonName"
        android:text=" You did it. "
        android:textColor="#0C0C0C"
        android:textColorHighlight="#191717"
        android:textColorHint="#0B0A0A"
        android:textColorLink="#040303"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.293" />

    <EditText
        android:id="@+id/editTextTextPersonName5"
        android:layout_width="583dp"
        android:layout_height="152dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="  Any regrets "
        android:textColor="#100F0F"
        android:textColorHighlight="#0E0D0D"
        android:textColorHint="#120F0F"
        android:textColorLink="#191717"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.627" />

</androidx.constraintlayout.widget.ConstraintLayout>